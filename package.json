{
  "name": "obvious-website-v2",
  "private": true,
  "description": "Give us an awwward already",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1",
    "prettier": "prettier --write \"./**/*.{js,css,json,md}\"",
    "storybook": "NODE_ENV=production start-storybook -s public",
    "build-storybook": "NODE_ENV=production build-storybook -s public",
    "start:lambda": "netlify-lambda serve functions",
    "build:lambda": "netlify-lambda build functions",
    "start-with-patch:lambda": "npm run patch && netlify-lambda serve functions",
    "build-with-patch:lambda": "npm run patch && netlify-lambda build functions",
    "patch": "mv node_modules/universal-user-agent/dist-web/ node_modules/universal-user-agent/dist-back && cp -vr node_modules/universal-user-agent/dist-node/ node_modules/universal-user-agent/dist-web"
  },
  "dependencies": {
    "core-js": "^2.6.5",
    "gatsby": "^2.17.4",
    "gatsby-plugin-offline": "^3.0.17",
    "gatsby-plugin-react-helmet": "^3.1.16",
    "gatsby-plugin-styled-components": "^3.1.11",
    "gatsby-source-storyblok": "^0.2.6",
    "netlify-lambda": "^1.6.3",
    "polished": "^3.4.2",
    "prop-types": "^15.7.2",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-helmet": "^5.2.1",
    "react-player": "^1.13.0",
    "storyblok-js-client": "^2.0.10",
    "storyblok-react": "0.0.10",
    "styled-components": "^4.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.7.4",
    "@octokit/rest": "^16.35.0",
    "@storybook/addon-actions": "^5.2.6",
    "@storybook/addon-links": "^5.2.6",
    "@storybook/addons": "^5.2.6",
    "@storybook/react": "^5.2.6",
    "babel-loader": "^8.0.6",
    "babel-plugin-remove-graphql-queries": "^2.7.16",
    "babel-plugin-styled-components": "^1.10.6",
    "eslint": "^6.1.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.6.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-react-hooks": "^1.7.0",
    "husky": "^3.1.0",
    "lint-staged": "^9.4.3",
    "lodash": "^4.17.15",
    "prettier": "1.19.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "./**/*.{js,css,json,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-hello-world"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  }
}
